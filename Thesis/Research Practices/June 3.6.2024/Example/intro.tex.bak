\section{Background and methodology}

\begin{frame}{Review of template-based BAO analyses}
	\begin{themedTitleBlock}{The Alcock-Paczynski effect}
		An incorrect catalogue cosmology choice breaks statistical isotropy.

		We \alert{identify the correct cosmology} by that keeping longitudinal and trasversal BAO scales equal. 
	\end{themedTitleBlock}
	\textbf{Idea}: Choose arbitrary catalogue cosmology $\Omega^c$. This induces anisotropic signal. 

	$\implies$ The distance to isotropy quantifies the distance to the true cosmology.
\end{frame}

\begin{frame}{Measuring anisotropy}
	To measure anisotropy, we refer to the main BAO distances, $D_H$ and $D_M$
	\begin{itemize}
	\item	$\Delta r_{\parallel} = D_H(z) \Delta z$, $D_H(z)$ the Hubble distance $D_H(z) = \frac{c}{H(z)}$
	\item$\Delta r_\perp = D_M(z) \Delta \theta$, $D_M(z)$ the angular size distance  $D_M(z) = \int_{0}^{z} \frac{cdz'}{H(z')}$
	\end{itemize}	
	With $H(z)$ the Hubble function 
	\begin{align*}
		H(z) &= H_0 \sqrt{\Omega_m (1+z)^3 + \Omega_k (1+z)^2 + \Omega_\Lambda}, \, \\
		H_0 &= H(z=0) = 100h \text{ km s} ^{-1} \text{Mpc} ^{-1}
	\end{align*}
\end{frame}

\begin{frame}{Scaling parameters}
	We quantify the distorsion of the signal with the variables
\begin{align}
	q_{\parallel} = \frac{D_H}{D_H^c}; \,\,\,\,\,\,\,\,\,\,
	q_{\perp} = \frac{D_M}{D_M^c},
\end{align}	

During inference, we scale the $q_{\parallel, \perp}$ parameters to fit the template BAO peak position.
\begin{align}
	\alpha_\parallel = \frac{D_H / r_d}{D_H^c/r_d^t}; \,\,\,\,\,\,\,
	\alpha_\perp = \frac{D_M / r_d}{D_M^c/r_d^t}
\end{align}
\end{frame}

\begin{frame}{The template and its multipoles}
	The power spectrum \alert{template} 
	\begin{align}
		P_{BAO}(k, \mu) = B(1+\beta_{eff}\mu) P^{sm}_{lin}(k) \left\{1 + [ \mathcal{O}_{lin}(k) - 1 ] e^{-\frac{1}{2}k^2\left[ \mu^2\Sigma_\parallel^2 + \left( 1-\mu^2 \right)\Sigma_\perp^2  \right] } \right\} 
	\end{align}
	\begin{itemize}
		\item $B$, $\beta_{eff}$ non-BAO nuisance parameters,
		\item $P_{lin}^{sm}$ the smoothed broadband component 
		\item $\mathcal{O}_{lin}$ the BAO component calculated as 
		\begin{align}
			\mathcal{O}_{lin} = \frac{P_{BAO}(k)}{P_{lin}^{sm}(k)}
		\end{align}
	\item $\Sigma_{\parallel}, \Sigma_{\perp}$ regulate the smoothing of the BAO oscillations
	\end{itemize}
\end{frame}

\begin{frame}{The template and its multipoles}
\begin{figure}[h]
	\centering
		\includegraphics[width=0.8\textwidth]{figs/pk_plin_olin.png}
	\caption{Components of the power spectrum }
	\label{fig:figs-}
\end{figure}	
\end{frame}


\begin{frame}{The template and its multipoles}
	We calculate the multipoles from the template following 
	\begin{align}
		P_{\text{BAO}}^{(\ell)}(k) = \frac{2\ell+1}{2}\int_{-1}^{1} d\mu P_{\text{BAO}}\left( k', \mu' \right) \mathcal{L}_\ell (\mu) + \sum_{i=0}^{n}A_i^{(\ell)}k^{2-i}
	\end{align}
With $k', \mu'$ the observed scale and line-of-sight modified by the $\alpha$	
\begin{align}
	k' = \frac{k}{\alpha_\perp} \left[ 1 + \mu^2\left( \frac{\alpha_{\perp}^2}{\alpha^2_{\parallel}}-1 \right)  \right] ^{\frac{1}{2}}, 
	\mu' = \mu\frac{\alpha_\perp}{\alpha_\parallel} \left[ 1 + \mu^2\left( \frac{\alpha_{\perp}^2}{\alpha^2_{\parallel}}-1 \right)  \right] ^{-\frac{1}{2}}
\end{align}
For $n=5$ ,$ \{A_i^{\left( 0,2 \right)} ,B\}_{NGC/SGC} + \beta_{\text{eff}}$ yield 23 nuisance parameters.
\end{frame}

