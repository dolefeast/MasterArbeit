	\end{align}
	This way, we have high enough z,
	\begin{enumerate}
		\item to see effect of catalogue cosmology choice
		\item to have large enough signal-to-noise ratio
	\end{enumerate}

	For the cosmological inference we use the full sample, with $		z \in \left( 0.2, 2.2 \right) $
	
\end{frame}

\begin{frame}{Cosmological parameter inference}
We fit power spectrum monopole and quadrupole in the range 
\begin{align}
	0.02 < k\left[ \text{h Mpc}^{-1} \right] <0.30, \Delta k = 0.01 \text{hMpc}^{-1}
\end{align}
The posterior distributions are sampled through MCMC with likelihood $\mathcal{L}$
\begin{align*}
	-2 \log \mathcal{L} \left( \mathbf{p} | \mathbf{\Omega}^t, \mathbf{\Omega}^c \right)  = \chi^2\left( \mathbf{p}|\Omega^t, \Omega^c \right) 
	&= \left[ D\left( \mathbf{\Omega}^c \right) - T\left( \mathbf{p}| \mathbf{\Omega}^t \right)  \right] \times\ldots \\
	&\ldots\times C^{-1}\left( \mathbf{\Omega}^c \right) \times \ldots\\
	& \ldots\times \left[ D\left( \mathbf{\Omega}^c \right) - T\left( \mathbf{p}|\mathbf{\Omega}^t \right)  \right] ^T
\end{align*}
With $D$ the data vector,  $T$ the model vector, $\textbf{p}$ the model's parameters and C the covariance matrix (from the mocks).
\end{frame}

