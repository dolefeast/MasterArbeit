\begin{frame}{BAO observables}
	
	\begin{itemize}[<+->]
		\item The \textbf{Overdensity} $\delta(\textbf{r}) = \frac{\rho(\textbf{r}) - \overline \rho}{\overline{\rho}}$ and its Fourier transform $$\tilde \delta(\textbf{k}) = \int \delta(\textbf{x}) e ^{-i \textbf{k} \cdot \textbf{x}}dx$$
	\item The \textbf{two point correlation function}:
		$\xi(R) = \left<\delta(\textbf{r}) \delta(\textbf{r}+\textbf{R}) \right>$
	\item The \textbf{power spectrum} $$ \left<\tilde\delta(\textbf{k})\tilde\delta^*(\textbf{k}') \right> = \left( 2\pi \right)^3 P(k) \delta^3(\textbf{k}-\textbf{k}')$$

	\end{itemize}
\end{frame}

